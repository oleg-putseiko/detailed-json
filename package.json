{
  "name": "detailed-json",
  "description": "Package that provides a type-safe and circularly-safe API for working with detailed JSON",
  "version": "1.0.4",
  "license": "MIT",
  "author": "Oleg Putseiko <oleg.putseiko@gmail.com> (https://github.com/oleg-putseiko)",
  "keywords": [
    "json",
    "error",
    "stringify",
    "parse",
    "circular"
  ],
  "homepage": "https://github.com/oleg-putseiko/detailed-json",
  "repository": {
    "type": "git",
    "url": "https://github.com/oleg-putseiko/detailed-json"
  },
  "bugs": {
    "url": "https://github.com/oleg-putseiko/detailed-json/issues"
  },
  "main": "dist/index.js",
  "files": [
    "dist/**/*"
  ],
  "exports": "./dist/index.js",
  "scripts": {
    "build": "yarn build:swc && yarn build:tsc",
    "build:swc": "swc src -d dist --strip-leading-paths",
    "build:tsc": "tsc",
    "lint": "eslint ./src/**/*.ts -c ./eslint.config.mjs",
    "lint:fix": "yarn lint --fix && yarn format:fix",
    "lint:strict": "yarn lint --max-warnings=0 && yarn format",
    "format": "prettier -c .",
    "format:fix": "prettier -w .",
    "test": "jest",
    "typecheck": "tsc --noEmit --incremental false",
    "prepare": "husky install | chmod +x ./.husky/*"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@swc/cli": "^0.5.2",
    "@swc/core": "^1.10.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.1",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^10.1.1",
    "husky": "^8.0.3",
    "jest": "^30.2.0",
    "lint-staged": "^15.0.2",
    "prettier": "^3.0.3",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "volta": {
    "node": "22.21.0",
    "yarn": "3.6.4"
  }
}
